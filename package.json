{
  "name": "rive-vue",
  "version": "4.21.4",
  "description": "Vue wrappers around the rive-js library",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "test:vue": "vitest",
    "build": "bunchee src/index.ts -m --no-sourcemap",
    "build:vue": "vite build --config vite.config.ts",
    "dev": "watch 'npm run build' src",
    "dev:vue": "vite build --config vite.config.ts --watch",
    "lint": "eslint -c .eslintrc.js 'src/**/*{.ts,.tsx}'",
    "lint:vue": "eslint -c .eslintrc.js 'src/**/*{.ts,.vue}'",
    "format": "prettier --write src",
    "format:vue": "prettier --write src",
    "types:check": "tsc --noEmit",
    "types:check:vue": "vue-tsc --noEmit",
    "release": "release-it",
    "release:patch": "npm run release -- --ci",
    "release:minor": "npm run release -- minor --ci",
    "release:major": "npm run release -- major --ci",
    "setup-builds": "./scripts/build.sh",
    "setup-packages": "./scripts/setup_all_packages.sh",
    "bump-versions": "./scripts/bump_all_versions.sh $npm_package_version",
    "publish:all": "./scripts/publish_all.sh --access public",
    "storybook": "cd examples-vue && yarn --cwd examples storybook",
    "demo:vue": "cd examples-vue && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rive-app/rive-react.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rive-app/rive-react/issues"
  },
  "homepage": "https://github.com/rive-app/rive-react#readme",
  "dependencies": {
    "@rive-app/canvas": "2.30.1",
    "@rive-app/canvas-lite": "2.30.1",
    "@rive-app/webgl": "2.30.1",
    "@rive-app/webgl2": "2.30.1"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@testing-library/jest-dom": "^5.13.0",

    "@types/jest": "^27.0.3",
    "@types/offscreencanvas": "^2019.6.4",

    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.5.0",
    "auto-changelog": "^2.3.0",
    "babel-loader": "^8.2.5",
    "bunchee": "^6.6.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",

    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-vue": "^9.33.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "release-it": "^19.0.4",
    "ts-jest": "^27.1.1",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vue": "^3.4.0",
    "vue-tsc": "^2.2.12",
    "watch": "^0.13.0"
  }
}
